# ISA SuperApp â€” Environment Example (do not commit real secrets)

# LLM runtime
ISA_LLM_PROVIDER=openrouter  # or: openai
ISA_FORBID_DIRECT_LLM=0      # set 1 to enforce orchestrator-only policy
OPENROUTER_API_KEY=
OPENAI_API_KEY=

# Memory & context
ISA_MEMORY_ADAPTERS=langchain,structured,memengine,zep,amem,aws,mcp
ISA_SLEEPTIME_MINUTES=30
ISA_MEMORY_FILE=./ISA_SuperApp/memory.json

# Observability
OTEL_ENABLED=0
OTEL_EXPORTER_OTLP_ENDPOINT=http://127.0.0.1:4318

# Services (optional)
SUPABASE_URL=
SUPABASE_ANON_KEY=
NEO4J_URI=neo4j://localhost:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=

# Auth/demo
ISA_AUTH_SECRET=change-me

