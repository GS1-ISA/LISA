# OpenRouter Configuration
# API key rotated - set OPENROUTER_API_KEY environment variable with new key

# Free Model Configuration (based on latest research)
OPENROUTER_USE_FREE_MODELS=true
OPENROUTER_PRIMARY_FREE_MODEL=google/gemini-2.5-flash-image-preview:free
OPENROUTER_FALLBACK_FREE_MODEL=meta-llama/llama-4-scout:free
OPENROUTER_FAST_FREE_MODEL=mistralai/mistral-small-3.1-24b-instruct:free
OPENROUTER_REASONING_FREE_MODEL=deepseek/deepseek-r1:free
OPENROUTER_BACKUP_FREE_MODEL=google/gemini-2.0-flash-exp:free

# Rate limiting for free tier
OPENROUTER_FREE_RATE_LIMIT=50
OPENROUTER_FREE_MAX_TOKENS=10000

# Existing configuration
# Set OPENAI_API_KEY and ANTHROPIC_API_KEY as environment variables for security
ALLOW_RESEARCH_LIVE=1
AGENT_WRITE=0
ADAPTER_STUB_MODE=1